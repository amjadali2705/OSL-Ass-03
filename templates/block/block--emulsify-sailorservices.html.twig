{% block content %}
  {% set sailor_services = [] %}


  {% for value in content["field_sailor_services_paragraph"]["#items"] %}
    {% set paragraph = value.entity %}
    {% set heading = paragraph.field_sailor_services_para_headi.value %}
    {% set subheading = paragraph.field_sailor_services_para_subhe.value %}
    {% set icon = paragraph.field_services_icon.value %}

    
    
    {% set sailor_services = sailor_services|merge([
      {
        'h4text': heading,
        'paragraph': subheading,
        'serviceitemicon': icon
      }
    ]) %}

  {% endfor %}

  {% include "@organisms/sailorservices/sailorservices.twig" with {
    sailorservices: sailor_services
  } %}
{% endblock %}

