<section id="features" class="features section">

      {% include "@molecules/sectionheading/sectionheading.twig" with {
        heading_level: '2',
        heading: heading,
        p_content: p_content
      } %}

      <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="row">
          <div class="col-lg-3">

            {# {% include "@molecules/featuretablists/featuretablists.twig" %} #}
            <ul class="nav nav-tabs flex-column" role="tablist">

              {% for data in tablistitemdata %}

                {% include "@atoms/tablistitem/tablistitem.twig" with {
                  isactive: data.isactive,
                  lihref: data.lihref,
                  areaselected: data.areaselected,
                  licontent: data.licontent
                } %}

              {% endfor %}        
              
            </ul>
           
          </div>

          <div class="col-lg-9 mt-4 mt-lg-0">
            <div class="tab-content">

                {% for data in featuretabsdata %}

                    {% include "@molecules/featuretabscontent/featuretabscontent.twig" with {
                        isactive: data.isactive,
                        tabid: data.tabid,
                        heading_level: data.heading_level,
                        heading: data.heading,
                        ptext: data.ptext,
                        p_content: data.p_content,
                        img_src: data.img_src
                    } %}

                {% endfor %}

            </div>
          </div>
        </div>

      </div>

    </section>