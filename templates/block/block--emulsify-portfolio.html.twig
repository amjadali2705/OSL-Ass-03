{% block content %}
  {% set sailor_portfolio = [] %}

  {% set portfolioheading = content.field_portfolio_heading['#items'][0].value %}
  {% set portfoliosubheading = content.field_portfolio_subheading['#items'][0].value %}

  {# {{dump(content.field_portfolio_subheading['#items'][0].value)}} #}

  {% for value in content["field_portfolio_paragraph"]["#items"] %}
    {% set paragraph = value.entity %}
    {% set portfolioparaheading = paragraph.field_portfolio_paragraph_headin.value %}
    {% set portfolioparasubheading = paragraph.field_portfolio_paragraph_subhea.value %}
    {% set img = paragraph.field_portfolio_paragraph_img.entity.uri.value %}
    {% set imgurl = file_url(img) %}

    
    {% set sailor_portfolio = sailor_portfolio|merge([
      {
        'h4content': portfolioparaheading,
        'img_src': imgurl,
        'pcontent': portfolioparasubheading

      }
    ]) %}

  {% endfor %}

  {% include "@organisms/portfolio/portfolio.twig" with {
    portfoliodata: sailor_portfolio, 
    heading: portfolioheading,
    p_content: portfoliosubheading
  } %}
{% endblock %}